@model Task_Announcement.Models.Announce
@{
    ViewBag.Title = "Create Announcement";
}

    <form asp-action="create" asp-controller="home">
        <div class="form-group">
            <label asp-for="Title" class="control-label">Title</label>
            <input type="text" asp-for="Title" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="Description" class="control-label">Discription</label>
            <input type="text" asp-for="Description" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="data" class="control-label">Data</label>
            <input type="text" asp-for="data" class="form-control" value="@DateTime.Today.ToString("d")" autocomplete="on"/>
        </div>
        <div class="form-group">
            <input type="submit" value="Enter" class="button" />
        </div>
    </form>